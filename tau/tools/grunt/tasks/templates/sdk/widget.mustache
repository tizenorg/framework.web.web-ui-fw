<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<html>
<head>
	<script type="text/javascript" src="../snippet.js"></script>
	<meta http-equiv="content-type" content="text/html; charset=utf-8">
	<link href="../styles.css" rel="StyleSheet" type="text/css">
	<link href="../snippet.css" rel="StyleSheet" type="text/css">
	<title>{{title}}{{version}}</title>
</head>
<body onload="prettyPrint()" id="content">
<h1>{{title}}</h1>


{{{brief}}}

{{#extends}}
<p>Widget extends: <a href="{{{extendsFile}}}">{{{extends}}}</a></p>
{{/extends}}

<h2>Table of Contents</h2>
<ol class="toc">
	{{#toc}}
		<li><a href="#{{href}}">{{name}}</a>
			{{#hasTOC}}
				<ol class="toc">
					{{#toc}}
						<li><a href="#{{href}}">{{name}}</a>
						</li>
					{{/toc}}
				</ol>
			{{/hasTOC}}
		</li>
	{{/toc}}
</ol>

{{{description}}}

{{#since}}
	<table class="note">
		<tbody>
		<tr>
			<th class="note">Note</th>
		</tr>
		<tr>
			<td class="note">This widget is supported since <strong>{{since}}</strong>.</td>
		</tr>
		</tbody>
	</table>
{{/since}}
{{#showOptions}}
	<h2><a id="options-list"></a>Options list</h2>

	<table>
		<tbody>
		<tr>
			<th>Option</th>
			<th>Input type</th>
			<th>Default value</th>
			<th>Description</th>
		</tr>
		{{#options}}
			<tr>
				<td class="option"><span style="font-family: Courier New,Courier,monospace">{{name}}</span></td>
				<td class="option">{{#types}}{{types}}{{/types}}</td>
				<td class="option">{{defaultValue}}</td>
				<td class="option">{{{description}}}</td>
			</tr>
		{{/options}}
		</tbody>
	</table>
{{/showOptions}}
{{#showEvents}}
	<h2><a id="events-list"></a>Events list</h2>

	<table>
		<tbody>
		<tr>
			<th>Name</th>
			<th>Description</th>
		</tr>
		{{#events}}
			<tr>
				<td class="option"><span style="font-family: Courier New,Courier,monospace">{{name}}</span></td>
				<td>{{{description}}}</td>
			</tr>
		{{/events}}
		</tbody>
	</table>

{{/showEvents}}
{{#showMethods}}
	<h2><a id="methods-list"></a>Methods list</h2>

	<h3>Summary</h3>
	<table class="informaltable">
		<thead>
		<tr>
			<th>Method</th>
			<th>Description</th>
		</tr>
		</thead>
		<tbody>
		{{#methods}}
			{{#isPublic}}
				<tr>
					<td>
						<pre class="intable prettyprint">{{#return.types}}{{return.types}} {{/return.types}}<a href="#method-{{name}}">{{name}}</a> ( {{#params}}{{types}}{{#isOptional}}?{{/isOptional}} {{name}}{{#defaultValue}} = {{defaultValue}}{{/defaultValue}}{{^isLast}}, {{/isLast}}{{/params}} ) </pre>
					</td>
					<td>{{{brief}}}</td>
				</tr>
			{{/isPublic}}
		{{/methods}}
		</tbody>
	</table>
	<dl>
		{{#methods}}
			{{#isPublic}}
				<dt class="method"><code><b><span class="methodName"
																   id="method-{{name}}">{{name}}</span></b></code></dt>
				<dd>
					<div class="brief">
						{{{brief}}}
					</div>
					<div class="synopsis">
						<pre class="signature prettyprint">{{#return.types}}{{return.types}} {{/return.types}}{{name}} ( {{#params}}{{types}}{{#isOptional}}?{{/isOptional}} {{name}}{{#defaultValue}} = {{defaultValue}}{{/defaultValue}}{{^isLast}}, {{/isLast}}{{/params}}) </pre>
					</div>
					{{#since}}
						<p><span class="version">Since: </span>
							{{since}}
						</p>
					{{/since}}
					<div class="description">
						<p>
							{{{description}}}
						</p>
					</div>
					{{#hasParams}}
					<div class="parameters">
						<p><span class="param">Parameters:</span></p>
					<table>
					<tbody>
					<tr>
						<th>Parameter</th>
						<th>Type</th>
						<th>Required / optional</th>
						<th>Default value</th>
						<th>Description</th>
					</tr>
					{{/hasParams}}
					{{#params}}
					<tr>
						<td><span style="font-family: Courier New,Courier,monospace">{{name}}</span></td>
						<td>{{types}}</td>
						<td>{{#isOptional}}optional{{/isOptional}}{{^isOptional}}required{{/isOptional}}</td>
						<td>{{defaultValue}}</td>
						<td>{{{description}}}</td>
					</tr>
					{{/params}}
					{{#hasParams}}
					</tbody>
					</table>
					</div>
					{{/hasParams}}
					<div class="returntype">
						<p><span class="return">Return value:</span></p>
						{{#return}}
							<table>
								<tbody>
								<tr>
									<th>Type</th>
									<th>Description</th>
								</tr>
								<tr>
									<td>{{types}}</td>
									<td>{{{description}}}</td>
								</tr>
								</tbody>
							</table>
						{{/return}}
						{{^return}}
							No Return Value
						{{/return}}
					</div>
					{{#examples}}
						<div class="example">
							<span class="example"><p>Code
								example:</p><p>{{name}}</p></span>
							<pre name="code" class="examplecode
							prettyprint">{{{code}}}</pre>
						</div>
					{{/examples}}
				</dd>
			{{/isPublic}}
		{{/methods}}
		</dl>
	{{/showMethods}}

<h2>Where to Go Next</h2>
<ul>
	<li><a href="../index.html">Widget Reference</a></li>
	{{#seeMore}}
		<li><a href="{{file}}">{{name}}</a></li>
	{{/seeMore}}
</ul>
