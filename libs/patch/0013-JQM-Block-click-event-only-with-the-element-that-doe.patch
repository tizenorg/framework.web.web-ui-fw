From b9404d316fb5efbf9fe7c8f3c5dfd02b373eb77d Mon Sep 17 00:00:00 2001
From: Youmin Ha <youmin.ha@samsung.com>
Date: Wed, 28 Nov 2012 16:14:24 +0900
Subject: [PATCH] JQM: Block click event only with the element that does
 preventDefault on vclick

Signed-off-by: Youmin Ha <youmin.ha@samsung.com>
---
 .../jquery-mobile-1.2.0/js/jquery.mobile.vmouse.js |    3 ++-
 1 file changed, 2 insertions(+), 1 deletion(-)

diff --git a/libs/js/jquery-mobile-1.2.0/js/jquery.mobile.vmouse.js b/libs/js/jquery-mobile-1.2.0/js/jquery.mobile.vmouse.js
index 666a4e6..25188d8 100644
--- a/libs/js/jquery-mobile-1.2.0/js/jquery.mobile.vmouse.js
+++ b/libs/js/jquery-mobile-1.2.0/js/jquery.mobile.vmouse.js
@@ -302,6 +302,7 @@ function handleTouchEnd( event ) {
 			t = getNativeEvent( event ).changedTouches[ 0 ];
 			clickBlockList.push({
 				touchID: lastTouchID,
+				target: event.target,
 				x: t.clientX,
 				y: t.clientY
 			});
@@ -496,7 +497,7 @@ if ( eventCaptureSupported ) {
 					o = clickBlockList[ i ];
 					touchID = 0;
 
-					if ( ( ele === target && Math.abs( o.x - x ) < threshold && Math.abs( o.y - y ) < threshold ) ||
+					if ( ( ele === target && target === o.target && Math.abs( o.x - x ) < threshold && Math.abs( o.y - y ) < threshold ) ||
 								$.data( ele, touchTargetPropertyName ) === o.touchID ) {
 						// XXX: We may want to consider removing matches from the block list
 						//      instead of waiting for the reset timer to fire.
-- 
